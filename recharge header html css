$(function(){
  
  const current = location.pathname;
  $('.orders-page').each(function(){
      const $this = $(this);
      // if the current path is like this link, make it active
      if($this.attr('href').indexOf(current) !== -1){
          $this.addClass('active');
      }
  })
  
  
})

</script>

